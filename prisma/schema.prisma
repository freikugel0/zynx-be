generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  password   String
  username   String   @unique
  fullName   String
  slug       String   @unique
  position   String?   
  phone      String?   
  photo      String?   
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
